{
    "rewrites": [
        {
            "source": "/dataApi/:path*",
            "destination": "https://apis.data.go.kr/:path*"
        },
        {
            "source": "/kobis/:path*",
            "destination": "https://kobis.or.kr/kobisopenapi/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}